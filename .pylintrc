[MASTER]
ignore=migrations,fixtures

[FORMAT]
max-line-length: 119

[REPORTS]
output-format=colorized

[MESSAGES CONTROL]
# Disable no-member because looks like it is being detected improperly.
# Disable invalid-name because it seem to mostly work incorrectly.
disable=no-member,invalid-name,missing-docstring,too-many-arguments,arguments-differ,too-few-public-methods,fixme,abstract-method,too-many-locals,too-many-ancestors
